<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">
                Detalles de la lectura
            </h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
        </div>
        <div>
            <div class="modal-body" id="modal-body">
                <table class="table">
                    <tr>
                        <th>Reloj</th>
                        <td>{{lecturareloj.reloj}}</td>
                    </tr>
                    <tr>
                        <th>Fecha</th>
                        <td>{{lecturareloj.fecha | date('d-m-Y H:i')}}</td>
                    </tr>
                    <tr>
                        <th>Lectura</th>
                        <td>{{lecturareloj.lectura}}</td>
                    </tr>
                    <tr>
                        <th>Usuario</th>
                        <td>{{lecturareloj.usuario}}</td>
                    </tr>
                </table>
            </div>
            <div class="modal-footer">
                <a class="btn btn-metal btn-sm" data-dismiss="modal"><i class="flaticon-cancel"></i> Cancelar</a>
                {% if eliminable==true%}
                    <a class="btn btn-danger btn-sm eliminar_lecturareloj" data-dismiss="modal" data-href="{{ path('lecturareloj_delete',{'id':lecturareloj.id}) }}"><i class="flaticon-delete-1"></i> Eliminar</a>
                {% endif %}
            </div>
        </div>
    </div>
</div>




