<div class="modal-dialog modal-full">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">
                Resultados: Kilómetros y litros consumidos
            </h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
        </div>
        <div>
            <div class="modal-body" id="modal-body">
                <div class="alert alert-info alert-dismissible fade show" role="alert">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"></button>
                    <b>Cantidad de kilómetros recorridos:</b> {{ data.totalkms }} <b>Total de litros:</b>{{ data.totallitros }}.
                </div>
                <table class="table">
                    <thead>
                    <tr>
                        <th>Matrícula</th>
                        <th>Kilómetros</th>
                        <th>Litros</th>
                    </tr>
                    </thead>
                    {% for obj in data.data %}
                        <tr>
                            <td>{{ obj.matricula }}</td>
                            <td>{{ obj.kms }}</td>
                            <td>{{ obj.litros }}</td>
                        </tr>
                    {% endfor %}
                    <tfoot>
                    <tr>
                        <th>Matrícula</th>
                        <th>Kilómetros</th>
                        <th>Litros</th>
                    </tr>
                    </tfoot>
                </table>
            </div>
            <div class="modal-footer">
                <a class="btn btn-metal" data-dismiss="modal"><i class="flaticon-cancel"></i> Cancelar</a>
                <a class="btn btn-primary exportar" data-dismiss="modal"><i class="flaticon-file"></i> Exportar</a>
            </div>
        </div>
    </div>
</div>


