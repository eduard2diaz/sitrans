<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">
                Detalles del municipio
            </h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
        </div>
        <div>
            <div class="modal-body" id="modal-body">
                <b>Municipio:</b> {{ municipio.nombre }}
                <div class="clearfix m--margin-bottom-20"></div>
                <table class="table" id="municipio_show">
                    <thead>
                    <tr>
                        <th>Instituciones</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for mun in municipio.institucions %}
                        <tr>
                            <td>{{ mun.nombre }}</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <a class="btn btn-metal btn-sm" data-dismiss="modal"><i class="flaticon-cancel"></i> Cancelar</a>
            </div>
        </div>
    </div>
</div>