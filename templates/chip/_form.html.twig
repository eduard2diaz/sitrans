{{form_start(form,{'attr':{
    'id': form_id|default('chip_new')
}})}}

<div class="modal-body" id="modal-body">
    <div class="row">
        <div class="col-md-4">
            {{form_row(form.tarjeta)}}
            {{form_errors(form)}}
        </div>
    </div>
    <div class="row margin-top-10">
        <div class="col-md-4">
            {{form_row(form.cupet)}}
        </div>
        <div class="col-md-4">
            {{form_row(form.numerocomprobante)}}
        </div>
        <div class="col-md-4">
            {{form_row(form.fecha)}}
        </div>

    </div>
    <div class="row margin-top-10">
        <div class="col-md-4">
            {{form_row(form.idfisico)}}
        </div>
        <div class="col-md-4">
            {{form_row(form.idlogico)}}
        </div>
    </div>
    <div class="row margin-top-10">
        <div class="col-md-4">
            {{form_row(form.moneda)}}
        </div>
        <div class="col-md-4">
            {{form_row(form.servicio)}}
        </div>
    </div>
    <div class="row margin-top-10">
        <div class="col-md-4">
            {{form_row(form.saldoinicial)}}
        </div>
        <div class="col-md-4">
            {{form_row(form.litrosextraidos)}}
        </div>
        <div class="col-md-4">
            {{form_row(form.importe)}}
        </div>
    </div>
    {{form_errors(form)}}
    {{form_rest(form)}}

</div>
<div class="modal-footer">
    <a class="btn btn-metal" data-dismiss="modal"><i class="flaticon-cancel"></i> Cancelar</a>
    <button class="btn btn-primary mt-ladda-btn ladda-button" data-style="expand-right" type="submit">
        <i class="flaticon-edit-1 ladda-label"></i> {{ action |default('Guardar')}}
        <span class="ladda-spinner"></span>
    </button>
</div>
{{form_end(form)}}

