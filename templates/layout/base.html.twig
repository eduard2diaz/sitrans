<!DOCTYPE html>
<html lang="en" >
<!-- begin::Head -->
<head>
    <meta charset="utf-8" />
    <title>
        {% block title %}SITRANS{% endblock title %}
    </title>
    <meta name="description" content="Latest updates and statistic charts">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
       /* WebFont.load({
            google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
        });*/
    </script>
    <!--end::Web font -->
    <!--begin::Base Styles -->
    <!--begin::Page Vendors -->
    <link href="{{ asset('vendors/custom/fullcalendar/fullcalendar.bundle.css','template')}}" rel="stylesheet" type="text/css" />
    <!--end::Page Vendors -->
    <link href="{{ asset('vendors/base/vendors.bundle.css','template')}}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('demo/demo9/base/style.bundle.css','template')}}" rel="stylesheet" type="text/css" />
    <!--end::Base Styles -->
    <link href="{{ asset('assets/sitrans/plugins/ladda/ladda-themeless.min.css')}}" rel="stylesheet" type="text/css" />
    <link rel="shortcut icon" href="{{ asset('assets/sitrans/img/icono.ico')}}" />
    <link rel="stylesheet" type="text/css"  href="{{ asset('assets/sitrans/css/base/sitrans.css')}}" />
    {% block stylesheets %}{% endblock stylesheets %}
</head>
<!-- end::Head -->
<!-- end::Body -->
<body  class="m--skin- m-content--skin-light m-header--fixed m-header--fixed-mobile m-aside-left--offcanvas-default m-aside-left--enabled m-aside-left--fixed m-aside-left--skin-dark m-aside--offcanvas-default"  >
<!-- begin:: Page -->
<div class="m-grid m-grid--hor m-grid--root m-page">

    <!-- BEGIN: Header -->
    {% block header %}
    {% endblock header %}
    <!-- END: Header -->

    <!-- begin::Body -->
    <div class="m-grid__item m-grid__item--fluid m-grid m-grid--hor-desktop m-grid--desktop m-body">
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-container m-container--responsive m-container--xxl m-container--full-height">
            <div class="m-grid__item m-grid__item--fluid m-wrapper">
                <!-- BEGIN: Subheader -->
                {% block subheader %}
                {% endblock subheader %}
                <!-- END: Subheader -->


                {% include 'layout/flashes/modal.html.twig' %}
                <div class="m-content">
                    {% block body %}
                    {% endblock body %}
                </div>
            </div>
        </div>
    </div>
    <!-- end:: Body -->
    <!-- begin::Footer -->
    {{ include('layout/recortes/footer.html.twig') }}
    <!-- end::Footer -->
</div>
<!-- end:: Page -->
<!--begin::Base Scripts -->
<script src="{{ asset('vendors/base/vendors.bundle.js','template')}}" type="text/javascript"></script>
<script src="{{ asset('demo/demo9/base/scripts.bundle.js','template')}}" type="text/javascript"></script>
<!--end::Base Scripts -->
<!--begin::Page Snippets -->
<script src="{{ asset('app/js/dashboard.js','template')}}" type="text/javascript"></script>
<script src="{{ asset('script/base/base.js','sitrans') }}"></script>
<!--end::Page Snippets -->
<!--cargando componenete ladda-->
<script src="{{ asset('assets/sitrans/plugins/ladda/spin.min.js') }}"></script>
<script src="{{ asset('assets/sitrans/plugins/ladda/ladda.min.js') }}"></script>
<!--fin de cargando componenete ladda-->

<!--Definiendo las configuraciones necesarias para JSRoutingBundle-->
<script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
<script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
<!--FIN de las configuraciones necesarias para JSRoutingBundle-->

<!-- begin::Page Loader -->
<script>
    $().ready(function(){
        base.init();
    })
</script>
<!-- end::Page Loader -->
{% block javascripts %}{% endblock javascripts %}
{% include 'layout/flashes/flashes.html.twig' %}
</body>
<!-- end::Body -->
</html>
